{
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".eslintignore": "ignore"
  },

  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,

  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    "src/dist": true,
    "src/main.prod.js": true,
    "src/main.prod.js.map": true,
    "bower_components": true,
    "dll": true,
    "release": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "test/**/__snapshots__": true,
    "yarn.lock": true,
    "*.{css,sass,scss}.d.ts": true
  },
  "workbench.colorCustomizations": {
    "activityBar.background": "#193211",
    "titleBar.activeBackground": "#060c04",
    "titleBar.activeForeground": "#e7e7e7",
    "activityBar.activeBackground": "#193211",
    "activityBar.activeBorder": "#415fbe",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#415fbe",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#060c04",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#193211",
    "titleBar.inactiveBackground": "#060c0499",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#060c04"
}
